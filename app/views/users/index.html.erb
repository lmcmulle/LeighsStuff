<h1>Listing users</h1>

<table>
  <tr>
    <th>Login</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Email</th>
    <th>Profile pic</th>
    <th>Bio</th>
    <th>Twitter</th>
    <th>Facebook</th>
    <th>Featured status</th>
    <th>Karma</th>
    <th>Openid identifier</th>
    <th>Twitter auth code</th>
    <th>Facebook auth code</th>
    <th>Crypted password</th>
    <th>Password salt</th>
    <th>Persistence token</th>
    <th>Single access token</th>
    <th>Perishable token</th>
    <th>Login count</th>
    <th>Failed login count</th>
    <th>Last request at</th>
    <th>Current login at</th>
    <th>Last login at</th>
    <th>Current login ip</th>
    <th>Last login ip</th>
    <th>Created at</th>
    <th>Updated at</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.login %></td>
    <td><%=h user.first_name %></td>
    <td><%=h user.last_name %></td>
    <td><%=h user.email %></td>
    <td><%=h user.profile_pic_id %></td>
    <td><%=h user.bio %></td>
    <td><%=h user.twitter_id %></td>
    <td><%=h user.facebook_id %></td>
    <td><%=h user.featured_status %></td>
    <td><%=h user.karma %></td>
    <td><%=h user.openid_identifier %></td>
    <td><%=h user.twitter_auth_code %></td>
    <td><%=h user.facebook_auth_code %></td>
    <td><%=h user.crypted_password %></td>
    <td><%=h user.password_salt %></td>
    <td><%=h user.persistence_token %></td>
    <td><%=h user.single_access_token %></td>
    <td><%=h user.perishable_token %></td>
    <td><%=h user.login_count %></td>
    <td><%=h user.failed_login_count %></td>
    <td><%=h user.last_request_at %></td>
    <td><%=h user.current_login_at %></td>
    <td><%=h user.last_login_at %></td>
    <td><%=h user.current_login_ip %></td>
    <td><%=h user.last_login_ip %></td>
    <td><%=h user.created_at %></td>
    <td><%=h user.updated_at %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user', new_user_path %>